@model IEnumerable<Library_MVC.Models.LiteratureType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            Name
        </th>
        <th>
            Genre
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TypeID })
        </td>
        <td>
			@item.Name
        </td>
        <td>
			@item.Genre
        </td>
    </tr>
}

</table>


